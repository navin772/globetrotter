{
  "version": 2,
  "builds": [
    {
      "src": "app/vercel_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/vercel_app.py"
    }
  ],
  "env": {
    "MONGODB_USERNAME": "@mongodb_username",
    "MONGODB_PASSWORD": "@mongodb_password",
    "SECRET_KEY": "@secret_key"
  }
}
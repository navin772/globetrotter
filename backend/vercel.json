{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "MONGODB_USERNAME": "@mongodb_username",
    "MONGODB_PASSWORD": "@mongodb_password",
    "SECRET_KEY": "@secret_key"
  }
}